{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@api/*": ["api/*"],
      "@components/*": ["components/*"],
      "@threejs/*": ["threejs/*"],
      "@pages/*": ["pages/*"]
    }
  },
  "include": ["src"]
}
